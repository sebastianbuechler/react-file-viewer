{
  "name": "@sbuechi/react-file-viewer",
  "version": "1.2.6",
  "description": "Extendable file viewer for web",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plangrid/react-file-viewer.git"
  },
  "type": "module",
  "scripts": {
    "dev": "webpack -d --watch",
    "build": "webpack -p --progress",
    "start": "node ./scripts/start.js",
    "lint": "node_modules/.bin/eslint ./src/**/*.jsx",
    "test": "node_modules/.bin/jest --env=jsdom --watch",
    "jest": "node_modules/.bin/jest --env=jsdom",
    "tag-and-publish": "node ./scripts/publish.js"
  },
  "author": "PlanGrid <opensource@plangrid.com>",
  "contributors": [
    {
      "name": "Alexei Schiopu",
      "email": "alexxschiopu@gmail.com",
      "url": "http://schiopu.me"
    },
    {
      "name": "Aaron Norby",
      "email": "aaron.norby@gmail.com"
    },
    {
      "name": "Viktoriya Savkina",
      "email": "viktoriya.savkina@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plangrid/react-file-viewer/issues"
  },
  "homepage": "https://github.com/plangrid/react-file-viewer#readme",
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-transform-class-properties": "^7.22.3",
    "@babel/plugin-transform-runtime": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@babel/runtime": "^7.22.3",
    "autoprefixer": "^9.8.8",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-transform-es2015-classes": "^6.18.0",
    "babel-plugin-transform-es2015-object-super": "^6.6.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chalk": "^5.2.0",
    "css-loader": "^6.8.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "inquirer": "^8.2.6",
    "jest": "^29.5.0",
    "node-sass": "^8.0.0",
    "postcss": "^8.4.24",
    "postcss-loader": "^7.3.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-test-renderer": "^16.6.3",
    "sass-loader": "^13.3.1",
    "style-loader": "^3.3.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.85.1",
    "webpack-dev-server": "^4.15.0"
  },
  "dependencies": {
    "comma-separated-values": "^3.6.4",
    "mammoth": "1.6.0",
    "pdfjs-dist": "3.7.107",
    "prop-types": "^15.8.1",
    "react-data-grid": "^6.0.1",
    "react-visibility-sensor": "^5.1.1",
    "three": "0.153.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.3/xlsx-0.19.3.tgz"
  },
  "peerDependencies": {
    "react": "18",
    "react-dom": "18"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|node_modules|scripts|example_files)[/\\\\]"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(css|scss)$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/tests/setupTests.js"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "plugins": [
      "import"
    ],
    "settings": {
      "import/resolver": {
        "webpack": "webpack.config.js"
      }
    }
  }
}
